{% extends 'layout.html.twig' %}

{% block content %}
    <div class="container">
        <table class="table">
            <tbody>
            <tr>
                <th>Email</th>
                <td>{{ user.email }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ user.Name }}</td>
            </tr>
            <tr>
                <th>FirstName</th>
                <td>{{ user.FirstName }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ user.Phone }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ user.Address }}</td>
            </tr>
            <tr>
                <th>Cp</th>
                <td>{{ user.Cp }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ user.City }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ user.Country }}</td>
            </tr>
            </tbody>
        </table>
    </div>

{% endblock %}
